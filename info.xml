<?xml version="1.0"?>
<extension key="io.compuco.financeexportformats" type="module">
  <file>financeexportformats</file>
  <name>CiviCRM Finance export formats</name>
  <description>CiviCRM Finance export formats</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Compuco</author>
    <email>hello@compuco.io</email>
  </maintainer>
  <urls>
    <url desc="Documentation">https://github.com/compucorp/io.compuco.financeexportformats/blob/master/readme.md</url>
  </urls>
  <releaseDate>2025-10-27</releaseDate>
  <version>8.21.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.51.3</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Financeexportformats</namespace>
    <angularModule>crmFinanceexportformats</angularModule>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Financeexportformats_Upgrader</upgrader>
</extension>
